import time
def is_valid_date(str):
    '''判断是否是一个有效的日期字符串'''
    if len(str) == 8:
        try:
            time.strptime(str, "%Y%m%d")
            return True
        except:
            return False
    else:
        return False
def count_after_point(str):
    """ 根据assert_stand里规定的小数位数，判断str是否正确"""

    s = str.split(".")
    count = len(s)
    if count != 2:
        sign = 'm'
    else:
        real_count = 0
        for x in s:
            if x.isdigit():
                real_count += 1
        if real_count == 2:
            sign='p{}'.format(len(s[1]))
        else:
            sign="m"
    return sign

