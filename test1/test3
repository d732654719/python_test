a="00+00+01"
sign=''
if "+" in a:
    s = a.split("+")
    count=len(s)
    if count > 2 or count < 2:
        sign = 'm'
    else:
        real_count=0
        for x in s:
            if x.isdigit():
                real_count+=1
        if real_count== 2:
            sign='+'
        else:
            sign='m'
print(sign)









